{
	"name": "offboarding",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
		"lint": "eslint --ignore-path .gitignore .",
		"prettier": "prettier --ignore-path .gitignore \"**/*.+(js)\"",
		"format": "yarn prettier --write",
		"check-format": "yarn prettier --list-different",
		"validate": "yarn check-format && yarn lint && yarn test && yarn build",
		"setup": "yarn install && yarn validate",
		"predev": "rm -rf .next",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"dockerized:build": "next build && next start",
		"dockerized:build-debug": "next build --debug && next start",
		"check-types": "tsc"
	},
	"dependencies": {
		"@geopagos/react-oz-wizard": "^2.0.1",
		"@material-ui/core": "^4.11.3",
		"@material-ui/icons": "^4.9.1",
		"axios": "^0.21.0",
		"clabe-js": "^1.0.1",
		"classnames": "^2.3.1",
		"formik": "^2.2.0",
		"https": "^1.0.0",
		"i18next": "^20.2.1",
		"lodash": "^4.17.21",
		"moment": "^2.29.1",
		"next": "10.1.3",
		"prop-types": "^15.7.2",
		"react": "^17.0.2",
		"react-device-detect": "^1.17.0",
		"react-dom": "17.0.2",
		"react-dropzone": "^11.3.2",
		"react-i18next": "^11.8.13",
		"react-imask": "^6.1.0",
		"react-node-to-string": "^0.1.1",
		"react-qr-code": "^2.0.2",
		"react-query": "^3.13.6",
		"react-sizeme": "^3.0.1",
		"react-text-mask": "^5.4.3",
		"rut.js": "^2.0.0",
		"typeface-roboto": "^1.1.13",
		"typescript": "^4.3.5",
		"validator": "^13.1.17",
		"yup": "^0.32.9"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^5.11.10",
		"@testing-library/react": "^11.2.6",
		"@testing-library/react-hooks": "^5.1.1",
		"@types/material-ui": "^0.21.9",
		"@types/react": "17.0.21",
		"babel-jest": "^26.6.0",
		"eslint": "^7.24.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-prettier": "^8.2.0",
		"eslint-import-resolver-webpack": "^0.13.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.1.2",
		"jest": "^26.6.0",
		"path": "^0.12.7",
		"prettier": "^2.1.2",
		"react-test-renderer": "17.0.2"
	}
}
