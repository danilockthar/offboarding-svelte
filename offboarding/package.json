{
	"name": "offboarding",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
		"lint": "eslint --ignore-path .gitignore .",
		"prettier": "prettier --ignore-path .gitignore \"**/*.+(js)\"",
		"format": "yarn prettier --write",
		"check-format": "yarn prettier --list-different",
		"validate": "yarn check-format && yarn lint && yarn test && yarn build",
		"setup": "yarn install && yarn validate",
		"predev": "rm -rf .next",
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"dockerized:build": "next build && next start",
		"dockerized:build-debug": "next build --debug && next start",
		"check-types": "tsc"
	},
	"dependencies": {
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@geopagos/react-oz-wizard": "^2.0.1",
		"@material-ui/icons": "^4.11.3",
		"@material-ui/core": "^4.11.3",
		"@mui/material": "^5.11.3",
		"axios": "^0.21.0",
		"classnames": "^2.3.1",
		"i18next": "^20.2.1",
		"next": "^13.1.1",
		"prop-types": "^15.7.2",
		"react": "^18.2.0",
		"react-device-detect": "^1.17.0",
		"react-dom": "^18.2.0",
		"react-query": "^3.39.2",
		"react-i18next": "^11.8.13",
		"react-sizeme": "^3.0.1"
	},
	"devDependencies": {
		"@types/node": "18.11.18",
		"typescript": "4.9.4",
		"babel-jest": "^26.6.0",
		"eslint": "^7.24.0",
		"eslint-config-airbnb": "^18.2.0",
		"eslint-config-prettier": "^8.2.0",
		"eslint-import-resolver-webpack": "^0.13.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.1.2",
		"jest": "^26.6.0",
		"path": "^0.12.7",
		"prettier": "^2.1.2",
		"react-test-renderer": "17.0.2"
		}
}
